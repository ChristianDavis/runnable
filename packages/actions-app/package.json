{
  "name": "@kinetic-io/actions-app",
  "version": "0.1.0",
  "sideEffects": false,
  "main": "build/index.js",
  "types": "app/types.ts",
  "files": [
    "app/types.ts",
    "app/api",
    "build",
    "public"
  ],
  "scripts": {
    "build": "remix build",
    "dev": "ACTIONS_BASE_URL='/' remix dev",
    "format": "prettier --write .",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "typecheck": "tsc --noEmit",
    "start": "remix-serve build",
    "start:mocks": "binode --require ./mocks -- @remix-run/serve:remix-serve build"
  },
  "dependencies": {
    "@remix-run/node": "^1.7.1",
    "@remix-run/react": "^1.7.1",
    "@remix-run/server-runtime": "^1.7.1",
    "react-dom": "^18.2.0",
    "react": "^18.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.19.1",
    "@chakra-ui/icons": "^2.0.10",
    "@chakra-ui/react": "^2.3.4",
    "@chakra-ui/styled-system": "^2.3.3",
    "@chakra-ui/system": "^2.2.11",
    "@emotion/react": "^11",
    "@emotion/server": "^11",
    "@emotion/styled": "^11",
    "@iconify/react": "^4.0.0",
    "@remix-run/dev": "^1.7.1",
    "@remix-run/eslint-config": "^1.7.1",
    "@remix-run/serve": "^1.7.1",
    "@types/node": "^17.0.41",
    "@types/react-dom": "^18.0.6",
    "@types/react": "^18.0.20",
    "@types/uuid": "^8.3.4",
    "binode": "^1.0.5",
    "cross-env": "^7.0.3",
    "eslint": "^8.23.1",
    "framer-motion": "^7.3.6",
    "npm-run-all": "^4.1.5",
    "react-remove-scroll": "^2.5.5",
    "stylis": "^4.1.2",
    "tiny-invariant": "^1.2.0",
    "ts-node": "^10.8.1",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^4.7.3",
    "uuid": "^9.0.0"
  }
}
