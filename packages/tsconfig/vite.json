{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Vite",
  "extends": "./base.json",
  "compilerOptions": {
    "lib": ["dom", "dom.iterable", "esnext"],
    "noEmit": true,
    "declaration": false,
    "declarationMap": false,
    "types": ["vite/client"],
    "jsx": "react-jsx"
  },
  "include": ["src", "next-env.d.ts"],
  "exclude": ["node_modules"]
}
